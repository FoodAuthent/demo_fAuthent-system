{
    "lang" : "org.foodauthent.codegen.FoodAuthentCodegen",
    "inputSpec" : "src-gen/api/foodauthent.yaml",
    "outputDir" : "../foodauthent-osgi/foodauthent-rest/",
    "modelPackage" : "org.foodauthent.model",
    "apiPackage"   : "org.foodauthent.rest.service",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "rest-service",
        "apiTemplateFile" : "rest/restService.mustache",
        "apiNamePattern" : "##name##RestService",
        "supportingFiles" : [
            {
                "templateFile": "rest/serviceUtil.mustache",
                "package" : "org.foodauthent.rest.service",
                "destinationFileName" : "ServiceUtil.java"
            }     
         ]
    }
}

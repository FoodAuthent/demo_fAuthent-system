{
    "lang" : "org.foodauthent.codegen.FoodAuthentCodegen",
    "inputSpec" : "src-gen/api/foodauthent.yaml",
    "outputDir" : "../foodauthent-osgi/foodauthent-common/",
    "modelPackage" : "NOT_GENERATED",
    "apiPackage"   : "NOT_GENERATED",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "common",
        "supportingFiles" : [
           {
                "templateFile" : "serviceExceptions.mustache",
                "destinationFileName" : "FAExceptions.java",
                "package" : "org.foodauthent.common.exception" 
            }    
         ]
    }
}